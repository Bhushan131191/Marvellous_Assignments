function Chkprime(no1: number): boolean 
{
    if (no1 <= 1) 
    {
      return false;
    }
  
    for (let i = 2; i < no1; i++) 
    {
      if (no1 % i === 0) 
      {
        return false;
      }
    }
    return true;
}
  
  var no1:number=11

    if (Chkprime(no1)) 
    {
    console.log("is a prime number:"+no1)
    } 
    else 
    {
    console.log("Is not a prime number:"+no1)
    }
  
